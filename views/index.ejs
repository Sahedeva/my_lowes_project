      <div class="carousel-caption">
        <h3>Bathroom</h3>
      </div>
    </div>
    <div class="item">
      <img class='car_img category_choice' alt='Kitchen' src="/images/kitchen.jpg">
      <div class="carousel-caption">
        <h3>Kitchen</h3>
      </div>
    </div>
