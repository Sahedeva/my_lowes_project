      <img class='car_img category_choice' alt='all' src="/images/all.jpg">
      <div class="carousel-caption">
        <h3>All</h3>
      </div>
    </div>
    

  <!-- Controls -->
    <a class="left carousel-control" href="#carousel-categories" role="button" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#carousel-categories" role="button" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>
