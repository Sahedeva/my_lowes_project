<% include ./partials/header %>


<div class='col-xs-11 col-xs-offset-1'>
<h3><%= name %>'s Projects</h3>
</div>
<div class='col-xs-10 col-xs-offset-2'>
<% for(j=0;j<project.length;j++) { %>
<h5><%= project[j].projectName %></h5>
<h5><%= project[j].projectStyle %></h5>
<h5><%= project[j].category %></h5>
</div>
<div>
<ul style='list-style:none' >
	<% for(i=0;i<project[j].album.length;i++) { %>

		<li>
			<p><%= project[j].album[i][2] %></p>
			<img class="img-rounded" style="width:375px; height:375px;" src="data:image/jpeg;base64,<%= project[j].album[i][0] %>">
			<p><%= project[j].album[i][1] %></p>
		

		</li>
	<% } %>
</ul>
<% } %>

</div>



<% include ./partials/footer %>
